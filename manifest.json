{
    "manifest_version": 2,
    "name": "Nikhil's launcher",
    "description": "A quick launcher to navigate to my favorite websites",
    "version": "1.0.0",
    "icons" : {"128": "icon_128.png" },
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "permissions" : ["activeTab"]
}